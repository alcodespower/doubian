<swiper style="height: 100%;width: 100%;" indicator-dots indicator-color="rgba(0,0,0,.3)" circular="true" autoplay="true" current="0" interval="5000" duration="500">
    <swiper-item wx:for="{{  subjects  }}" wx:key="{{  item.id  }}" style="flex: 1;">
        <image src="{{  item.images.large  }}" mode="aspectFill" style="position:absolute;height: 100%;width: 100%;opacity: .8;" />
    </swiper-item>
</swiper>